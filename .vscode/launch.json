{
    // Use IntelliSense to find out which attributes exist for C# debugging
    // Use hover for the description of the existing attributes
    // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch UI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build UI",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceRoot}/iTunes.Editor.Avalonia/bin/Debug/netcoreapp3.1/iTunes.Editor.Avalonia.dll",
            "args": [],
            "cwd": "${workspaceRoot}",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Load XML",
            "preLaunchTask": "Build Console",
            "type": "coreclr",
            "request": "launch",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/iTunes.Editor.Console/bin/Debug/netcoreapp3.1/iTunes.Editor.Console.dll",
            "args": [
                "list",
                "%OneDriveConsumer%\\Music\\iTunes\\iTunes Library.xml",
                "--type",
                "plist"
            ],
            "cwd": "${workspaceFolder}/iTunes.Editor.Console",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Get Lyrics",
            "preLaunchTask": "Build Console",
            "type": "coreclr",
            "request": "launch",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/iTunes.Editor.Console/bin/Debug/netcoreapp3.1/iTunes.Editor.Console.dll",
            "args": [
                "lyrics",
                "Pink Floyd",
                "Comfortably Numb"
            ],
            "cwd": "${workspaceFolder}/iTunes.Editor.Console",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Get Composers",
            "preLaunchTask": "Build Console",
            "type": "coreclr",
            "request": "launch",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/iTunes.Editor.Console/bin/Debug/netcoreapp3.1/iTunes.Editor.Console.dll",
            "args": [
                "composer",
                "Pink Floyd",
                "Comfortably Numb"
            ],
            "cwd": "${workspaceFolder}/iTunes.Editor.Console",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Update XML",
            "preLaunchTask": "Build Console",
            "type": "coreclr",
            "request": "launch",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/iTunes.Editor.Console/bin/Debug/netcoreapp3.1/iTunes.Editor.Console.dll",
            "args": [
                "update",
                "list",
                "all",
                "%OneDriveConsumer%\\Music\\iTunes\\iTunes Library.xml",
                "--type",
                "plist"
            ],
            "cwd": "${workspaceFolder}/iTunes.Editor.Console",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Update Folder",
            "preLaunchTask": "Build Console",
            "type": "coreclr",
            "request": "launch",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/iTunes.Editor.Console/bin/Debug/netcoreapp3.1/iTunes.Editor.Console.dll",
            "args": [
                "update",
                "list",
                "all",
                "%OneDriveConsumer%\\Music\\iTunes\\iTunes Media\\Music\\Billie Eilish\\When We All Fall Asleep, Where Do We Go_",
                "--type",
                "folder"
            ],
            "cwd": "${workspaceFolder}/iTunes.Editor.Console",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}